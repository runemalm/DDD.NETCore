{
  "author": "David Runemalm",
  "name": "OpenDDD.NET Template Pack",
  "description": "A collection of templates for creating OpenDDD.NET solutions.",
  "identity": "OpenDDD.NET Template Pack",
  "shortName": "opendddnet-sln-netcore31",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MyBoundedContext",
  "symbols": {
    "framework": {
      "type": "parameter",
      "description": "The target framework for the solution.",
      "datatype": "choice",
      "enableQuotelessLiterals": true,
      "isRequired": false,
      "choices": [
        {
          "choice": "netcoreapp3.1",
          "description": "Target .NET Core 3.1"
        },
        {
          "choice": "net5.0",
          "description": "Target .NET 5"
        }
      ],
      "replaces": "$targetFramework$",
      "defaultValue": "netcoreapp3.1"
    }
  },
  "preferNameDirectory": true
}
